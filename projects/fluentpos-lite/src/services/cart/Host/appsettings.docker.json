{
  "App": {
    "Name": "cart-service"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuthOptions": {
    "Authority": "http://identity",
    "Audience": "cart-api"
  },
  "MongoOptions": {
    "ConnectionString": "mongodb://mongo",
    "DatabaseName": "cart-db"
  },
  "ELKOptions": {
    "Uri": "http://elk:9200"
  }
}